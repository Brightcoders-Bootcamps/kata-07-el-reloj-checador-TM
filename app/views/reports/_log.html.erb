<h3>Check Log</h3>
<% @reports.each do |report| %>
  <ul class="list-group list-group-horizontal">
    <li class="list-group-item"><%= report.employee_id %></li>
    <li class="list-group-item"><%= report.check_type %></li>
    <li class="list-group-item"><%= report.check_time %></li>
  </ul>
<% end %>